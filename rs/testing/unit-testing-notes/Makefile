.PHONY: dev dev-build build test

# creates (and runs) executable in ./target/debug/<project-name>
dev: 
	@ cargo run

# creates executable in ./target/debug/<project-name>
dev-build: 
	@ cargo build

# creates executable in ./target/release/<project-name>
build: 
	@ cargo build --release

test:
	@ cargo test
